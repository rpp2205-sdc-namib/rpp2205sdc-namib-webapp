{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.js",
  "scripts": {
    "test": "jest --env=jsdom ----coverage",
    "test:review": "jest ./client/src/components/Ratings_Reviews/rr.spec.js --env=jsdom",
    "test:overview": "jest ./client/src/components/Overview/overview.test.js --env=jsdom --detectOpenHandles ----coverage",
    "test:questions_answers": "jest ./client/src/components/Questions_Answers/questions_answers.test.js",
    "test:related": "jest ./client/src/components/RelatedItems_Comparison/related-items.test.js --env=jsdom --detectOpenHandles ----coverage",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production && nodemon ./server/index.js",
    "watch": "webpack --watch & nodemon ./server/index.js"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "json",
      "html"
    ],
    "verbose": true,
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpp2205-fec-session/rpp2205-session-atelier.git"
  },
  "author": "Brandon, Stacey, Yui, Sijia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rpp2205-fec-session/rpp2205-session-atelier/issues"
  },
  "homepage": "https://github.com/rpp2205-fec-session/rpp2205-session-atelier#readme",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@webpack-cli/generators": "^2.5.0",
    "babel-jest": "^29.1.2",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jest-puppeteer": "^6.1.1",
    "msw": "^0.47.4",
    "nodemon": "^2.0.20",
    "puppeteer": "^18.2.1",
    "react-icons": "^4.6.0",
    "react-test-renderer": "^18.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "react-router-dom": "^6.4.2",
    "spdy": "^4.0.2",
    "terser-webpack-plugin": "^5.3.6"
  }
}
